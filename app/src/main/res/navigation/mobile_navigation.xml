<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <activity
        android:id="@+id/navigation_home"
        android:name="com.example.sayurin.LoginActivity"
        android:label="@string/title_home"
        tools:layout="@layout/activity_login" />

    <activity
        android:id="@+id/navigation_dashboard"
        android:name="com.example.sayurin.MainActivity"
        android:label="@string/title_dashboard"
        tools:layout="@layout/activity_main" />

    <activity
        android:id="@+id/navigation_notifications"
        android:name="com.example.sayurin.OrderActivity"
        android:label="@string/title_order"
        tools:layout="@layout/activity_order" />
</navigation>
